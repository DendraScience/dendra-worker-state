{
    "source_defaults":
    {
        "queue_group": "decodePseudoBinary",
        "sub_options":
        {
            "ack_wait": 60000,
            "durable_name": "20250725"
        }
    },
    "sources":
    [
        {
            "description": "CSU: Decode DCP messages imported from GOES",
            "error_subject": "csu.decodePseudoBinary.v1.err.import.goes",
            "preprocessing_expr":
            [
                "($org := context.org_slug ~> $safeName;",
                "$station := context.station ~> $safeName;",
                "$table := context.table ~> $safeName;",
                "$tags := ['org' & '$' & $org, 'source$goes', 'station' & '$' & $station, 'table' & '$' & $table];",
                "$time := payload.header.timeDate;",
                "$params := {'tags': $tags, 'time': $time};",
                "$ ~> |$|{'params': $params, 'payload': payload.body}|;)"
            ],
            "pub_to_subject": "csu.decodePseudoBinary.v1.out",
            "sub_to_subject": "csu.import.v1.out.goes"
        }
    ],
    "static_rules":
    [
        {
            "begins_at": "2000-01-01T00:00:00.000Z",
            "definition":
            {
                "decode_columns":
                [
                    "Day_of_Year",
                    "HourMin",
                    "RS_kw_m2_Avg",
                    "RS_kw_m2_Max",
                    "RS_kw_m2_Min",
                    "RS_kw_m2_Std",
                    "PAR_Avg",
                    "PAR_Max",
                    "PAR_Min",
                    "mean_wind_vel",
                    "mean_wind_vector_mag",
                    "mean_vector_wind_dir",
                    "sd_wind_dir",
                    "WS_ms_Max",
                    "WS_ms_Min",
                    "WS_ms_Std",
                    "AT_C_Max",
                    "AT_C_Min",
                    "AT_C_Avg",
                    "RH_pct_Max",
                    "RH_pct_Min",
                    "RH_pct_Avg",
                    "BP_mB_GOES_Avg",
                    "PCPN_intrvl_Tot",
                    "PCPN_accumulated",
                    "RGTemp_C_Avg",
                    "AT_10_C_Max",
                    "AT_10_C_Min",
                    "AT_10_C_Avg",
                    "delta_T_C_Max",
                    "delta_T_C_Min",
                    "delta_T_C_Avg",
                    "Soil_T50_C_Max",
                    "Soil_T50_C_Min",
                    "Soil_T50_C_Avg",
                    "Soil_T100_C_Max",
                    "Soil_T100_C_Min",
                    "Soil_T100_C_Avg",
                    "Soil_T200_C_Max",
                    "Soil_T200_C_Min",
                    "Soil_T200_C_Avg",
                    "Soil_T500_C_Max",
                    "Soil_T500_C_Min",
                    "Soil_T500_C_Avg",
                    "VWC_50_Avg",
                    "VWC_V_Avg",
                    "Batt_volt_Max",
                    "Batt_volt_Min",
                    "Batt_volt_Avg",
                    "PTemp_Avg",
                    "Sta_ID"
                ],
                "decode_format": "fp2_51",
                "decode_slice":
                [
                    1,
                    919
                ],
                "time_edit": "so_h",
                "time_interval": 900
            },
            "ends_before": "2100-01-01T00:00:00.000Z",
            "tags":
            [
                "org$csu",
                "source$goes",
                "station$zzyzx"
            ]
        }
    ]
}