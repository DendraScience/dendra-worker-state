{
  "source_defaults": {
    "queue_group": "archive",
    "sub_options": {
      "ack_wait": 20000,
      "durable_name": "20210524"
    }
  },
  "sources": [
    {
      "description": "UCANR: Archive records imported from CSI",
      "error_subject": "ucanr.archive.v1.err.import.csi",
      "preprocessing_expr": [
        "($station := payload.station ~> $safeName;",
        "$table := payload.table ~> $safeName;",
        "$time := payload.timeString;",
        "$recNum := $pad($string(payload.recordNumber), -10, '0');",
        "$docId := $join(['ucanr', 'csi', $station, $table, $substring($time, 0, 10), $substring($time, 11, 2), $substring($time, 14, 2)], '-') & '_' & $recNum;",
        "$valid := $contains($docId, /^ucanr-csi-\\w+-\\w+-\\d{4}-\\d{2}-\\d{2}-\\d{2}-\\d{2}_\\d{10}$/);",
        "$params := $valid ? {'document_id': $docId} : {};",
        "$ ~> |$|{'params': $params}|;)"
      ],
      "sub_to_subject": "ucanr.import.v1.out.csi"
    }
  ]
}