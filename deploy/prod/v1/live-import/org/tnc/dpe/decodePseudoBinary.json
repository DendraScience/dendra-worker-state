{
    "source_defaults":
    {
        "queue_group": "decodePseudoBinary",
        "sub_options":
        {
            "ack_wait": 60000,
            "durable_name": "20210524"
        }
    },
    "sources":
    [
        {
            "description": "TNC: Decode DCP messages imported from GOES",
            "error_subject": "tnc.decodePseudoBinary.v1.err.import.goes",
            "preprocessing_expr":
            [
                "($org := context.org_slug ~> $safeName;",
                "$station := context.station ~> $safeName;",
                "$table := context.table ~> $safeName;",
                "$tags := ['org' & '$' & $org, 'source$goes', 'station' & '$' & $station, 'table' & '$' & $table];",
                "$time := payload.header.timeDate;",
                "$params := {'tags': $tags, 'time': $time};",
                "$ ~> |$|{'params': $params, 'payload': payload.body}|;)"
            ],
            "pub_to_subject": "tnc.decodePseudoBinary.v1.out",
            "sub_to_subject": "tnc.import.v1.out.goes"
        }
    ],
    "static_rules":
    [
        {
            "begins_at": "1500-01-01T00:00:00.000Z",
            "definition":
            {
                "decode_columns":
                [
                    "Day_of_Year",
                    "HourMin",
                    "RS_kw_m2_Avg",
                    "RS_kw_m2_Max",
                    "RS_kw_m2_Min",
                    "RS_kw_m2_Std",
                    "mean_wind_vel",
                    "mean_wind_vector_mag",
                    "mean_vector_wind_dir",
                    "sd_wind_dir",
                    "WS_ms_Max",
                    "WS_ms_Min",
                    "WS_ms_Std",
                    "AT_C_Max",
                    "AT_C_Min",
                    "AT_C_Avg",
                    "RH_pct_Max",
                    "RH_pct_Min",
                    "RH_pct_Avg",
                    "BP_mB_GOES_Avg",
                    "PCPN_intrvl_Tot",
                    "PCPN_accumulated",
                    "LWMDry_Tot",
                    "LWMCon_Tot",
                    "LWMWet_Tot",
                    "SMoist_1_Avg",
                    "SCondTcor_1_Avg",
                    "STempC_1_Avg",
                    "SMoist_2_Avg",
                    "SCondTcor_2_Avg",
                    "STempC_2_Avg",
                    "SMoist_3_Avg",
                    "SCondTcor_3_Avg",
                    "STempC_3_Avg",
                    "Batt_volt_Avg",
                    "PTemp_Max",
                    "Sta_ID"
                ],
                "decode_format": "fp2_37",
                "decode_slice":
                [
                    1,
                    667
                ],
                "time_edit": "so_h",
                "time_interval": 600
            },
            "ends_before": "2100-01-01T00:00:00.000Z",
            "tags":
            [
                "org$tnc",
                "source$goes"
            ]
        }
    ]
}