{
  "metrics": {
    "expiry_seconds": 86400
  },
  "source_defaults": {
    "metrics_groups": {
      "uploads": "context.upload_id"
    },
    "sub_options": {
      "ack_wait": 120000,
      "durable_name": "20200916",
      "max_in_flight": 1000,
      "start_at_time_delta": 0
    }
  },
  "sources": [
    {
      "description": "PREP: Write prepared points to Influx",
      "error_subject": "prep.influxWrite.v1.err.prep",
      "ignore_errors": true,
      "sub_to_subject": "prep.prep.v1.out",
      "writer_options": {
        "batch_interval": 5000,
        "batch_size": 500
      }
    }
  ]
}